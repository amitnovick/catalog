{
  "manifest_version": 2,
  "name": "Catalog Web Clipper (dev)",
  "version": "0.0.1",
  "description": "Save web clippings to Catalog app",
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "homepage_url": "https://github.com/amitnovick/catalog",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["activeTab", "tabs", "http://*/", "https://*/", "<all_urls>", "contextMenus"],
  "background": {
    "scripts": ["node_modules/webextension-polyfill/dist/browser-polyfill.js", "background.js"]
  }
}
